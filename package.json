{
  "name": "ccc-v2-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "localdev":"ng serve",
    "start": "ng serve --aot --open --port 2020",
    "dev-build": "ng build --configuration=dev --base-href=/dev/dist/ --output-path=dev/dist --build-optimizer",
    "dev-init": "sudo pm2 stop ui-ccc-v2-dev; sudo pm2 delete ui-ccc-v2-dev; sudo pm2 start server.dev.js --watch --name ui-ccc-v2-dev",
    "dev": "git pull; sudo npm install express; npm run dev-init",
    "uat-build": "ng build --configuration=uat --base-href=/ccc/ui/v2/uat/dist/ --output-path=uat/dist --build-optimizer",
    "uat-init": "sudo pm2 stop ui-ccc-v2-uat; sudo pm2 delete ui-ccc-v2-uat; sudo pm2 start server.uat.js --watch --name ui-ccc-v2-uat",
    "uat": "git pull; sudo npm install express; npm run uat-init",
    "prod-build": "ng build --configuration=production --base-href=/ccc/ui/v2/dist/ --output-path=dist --build-optimizer",
    "prod-init": "sudo pm2 stop ui-ccc-v2-prod; sudo pm2 delete ui-ccc-v2-prod; sudo pm2 start server.prod.js --watch --name ui-ccc-v2-prod",
    "prod": "sudo git pull; sudo npm install express; npm run prod-init"
  },
  "private": true,
